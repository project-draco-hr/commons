@classmethod
def setUpClass(cls):
    super(PythonDependenciesTests, cls).setUpClass()
    cls.define_target('dependencies', 'python_leaf')
    cls.define_target('dependencies', 'python_inner', deps=[pants('dependencies:python_leaf')])
    cls.define_target('dependencies', 'python_inner_with_external', deps=[python_requirement('antlr_python_runtime==3.1.3')])
    cls.define_target('dependencies', 'python_root', deps=[pants('dependencies:python_inner'), pants('dependencies:python_inner_with_external')])
