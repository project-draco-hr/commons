def get_base_classpath_for_target(self, target):
    'Note: to use this method, the exclusives_groups data product\n    must be available. This should have been set by the prerequisite\n    java/scala compile.'
    egroups = self.context.products.get_data('exclusives_groups')
    group_key = egroups.get_group_key_for_target(target)
    return egroups.get_classpath_for_group(group_key)
