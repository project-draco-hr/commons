{
  parse(StandardArgs.class,"-string=");
  assertThat(StandardArgs.stringVal.get(),is(""));
  resetArgs(StandardArgs.class);
  parse(StandardArgs.class,"-string=''");
  assertThat(StandardArgs.stringVal.get(),is(""));
  resetArgs(StandardArgs.class);
  parse(StandardArgs.class,"-string=\"\"");
  assertThat(StandardArgs.stringVal.get(),is(""));
}
