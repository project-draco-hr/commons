{
  assertThat(ArgScanner.joinKeysToValues(""),is(Arrays.asList("")));
  assertThat(ArgScanner.joinKeysToValues("-a","b","-c","-d"),is(Arrays.asList("-a=b","-c","-d")));
  assertThat(ArgScanner.joinKeysToValues("-a='b'","-c","-d","'e'"),is(Arrays.asList("-a='b'","-c","-d='e'")));
  assertThat(ArgScanner.joinKeysToValues("-a=-b","c","-d","\"e\""),is(Arrays.asList("-a=-b","c","-d=\"e\"")));
}
