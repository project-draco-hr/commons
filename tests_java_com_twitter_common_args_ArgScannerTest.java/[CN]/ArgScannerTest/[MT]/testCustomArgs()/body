{
  test(CustomArgs.class,new Command(){
    @Override public void execute(){
      assertThat(CustomArgs.nameVal.get(),is(new Name("jane")));
    }
  }
,"custom1","jane");
  test(CustomArgs.class,new Command(){
    @Override public void execute(){
      assertThat(CustomArgs.meaningVal.get(),is(new MeaningOfLife(42L)));
    }
  }
,"custom2","jim");
}
