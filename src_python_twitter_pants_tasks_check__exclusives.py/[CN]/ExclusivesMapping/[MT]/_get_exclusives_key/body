def _get_exclusives_key(self, target):
    target_key = []
    for k in self.conflicting_exclusives:
        if (len(target.exclusives[k]) > 0):
            target_key.append(('%s=%s' % (k, list(target.exclusives[k])[0])))
        else:
            target_key.append(('%s=<none>' % k))
    if (target_key == []):
        return '<none>'
    else:
        return ','.join(target_key)
