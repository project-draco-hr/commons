def jardep(self, jar, classifier=None):
    return TemplateData(org=jar.org, name=(jar.name + ('-only' if (classifier == 'idl') else '')), rev=jar.rev, scope=('runtime' if (classifier == 'idl') else 'compile'), classifier=(classifier if (classifier is not None) else jar.classifier), excludes=[self.create_exclude(exclude) for exclude in jar.excludes if exclude.name])
