@classmethod
def iter_entries(cls, stream):
    '\n\t\t:return: Iterator yielding RefLogEntry instances, one for each line read \n\t\t\tsfrom the given stream.\n\t\t:param stream: file-like object containing the revlog in its native format\n\t\t\tor basestring instance pointing to a file to read'
    new_entry = RefLogEntry.from_line
    if isinstance(stream, basestring):
        stream = file_contents_ro_filepath(stream)
    while True:
        line = stream.readline()
        if (not line):
            return
        yield new_entry(line.strip())
