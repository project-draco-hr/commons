def __del__(self):
    'Write pending changes if required and release locks'
    if (self.read_only or (not self._lock._has_lock())):
        return
    try:
        try:
            self.write()
        except IOError as e:
            print ('Exception during destruction of GitConfigParser: %s' % str(e))
    finally:
        self._lock._release_lock()
