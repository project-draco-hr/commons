def profile_classpath(self, profile):
    'Ensures the classpath for the given profile ivy.xml is available and returns it as a list of\n    paths.\n\n    profile: The name of the tool profile classpath to ensure.\n    '

    def java_runner(main, classpath=None, opts=None, args=None, jvmargs=None, workunit_factory=None, workunit_name=None):
        assert (workunit_factory is None)
        return self.runjava_indivisible(main, classpath=classpath, opts=opts, args=args, jvmargs=jvmargs, workunit_name=workunit_name)
    return binary_util.profile_classpath(profile, java_runner=java_runner, config=self.context.config)
