def _help(root_dir):
    print ('Pants %s @ BUILD_ROOT: %s' % (_VERSION, root_dir))
    print 
    print ('Available subcommands:\n\t%s' % '\n\t'.join(find_all_commands()))
    print 
    print "Default subcommand flags can be stored in ~/.pantsrc using the 'options' key of a\nsection named for the subcommand in ini style format, ie:\n  [build]\n  options: --fast"
    exit()
