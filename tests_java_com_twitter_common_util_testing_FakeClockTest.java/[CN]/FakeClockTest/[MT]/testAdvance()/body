{
  fakeTicker.advance(Amount.of(42L,Time.NANOSECONDS));
  assertEquals(42L,fakeTicker.read());
  fakeTicker.advance(Amount.of(42L,Time.NANOSECONDS));
  assertEquals(84L,fakeTicker.read());
  fakeTicker.advance(Amount.of(-42L,Time.NANOSECONDS));
  assertEquals(42L,fakeTicker.read());
  fakeTicker.advance(Amount.of(-43L,Time.NANOSECONDS));
  assertEquals(-1L,fakeTicker.read());
}
