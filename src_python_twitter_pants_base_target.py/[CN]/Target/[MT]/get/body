@classmethod
def get(cls, address):
    'Returns the specified module target if already parsed; otherwise, parses the buildfile in the\n    context of its parent directory and returns the parsed target.\n    '

    def lookup():
        return cls._targets_by_address.get(address, None)
    target = lookup()
    if target:
        return target
    else:
        ParseContext(address.buildfile).parse()
        return lookup()
