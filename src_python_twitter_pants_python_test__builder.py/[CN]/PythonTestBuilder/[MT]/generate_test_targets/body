@staticmethod
def generate_test_targets():
    if (PythonTestBuilder.TESTING_TARGETS is None):

        def define_targets():
            return [PythonRequirement('pytest'), PythonRequirement('unittest2', version_filter=(lambda : (sys.version_info[0] == 2))), PythonRequirement('unittest2py3k', version_filter=(lambda : (sys.version_info[0] == 3)))]
        PythonTestBuilder.TESTING_TARGETS = ParseContext.fake(define_targets)
    return PythonTestBuilder.TESTING_TARGETS
