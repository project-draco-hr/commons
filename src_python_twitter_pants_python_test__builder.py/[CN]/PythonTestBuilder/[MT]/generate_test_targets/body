@classmethod
def generate_test_targets(cls):
    if (cls.TESTING_TARGETS is None):
        with ParseContext.temp():
            cls.TESTING_TARGETS = [PythonRequirement('pytest'), PythonRequirement('pytest-cov'), PythonRequirement('unittest2', version_filter=(lambda py, pl: py.startswith('2'))), PythonRequirement('unittest2py3k', version_filter=(lambda py, pl: py.startswith('3')))]
    return cls.TESTING_TARGETS
