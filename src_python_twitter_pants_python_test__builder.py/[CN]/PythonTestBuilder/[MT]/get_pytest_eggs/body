@staticmethod
def get_pytest_eggs(root):
    specs = ['3rdparty/python:pytest', '3rdparty/python:py']
    eggs = []
    for spec in specs:
        address = Address.parse(root, spec)
        target = Target.get(address)
        for dep in target.dependencies:
            for egg in dep.eggs:
                eggs.append(egg)
    return eggs
