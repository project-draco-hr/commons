@staticmethod
def from_file(filename, decode_full=True):
    '\n    Parameters:\n      filename: the filename of the class file.\n      decode_full: optimization flag. If True, the entire classfile is decoded.\n         If false, then decoding stops once the constant pool has been decoded.\n    '
    with open(filename, 'rb') as fp:
        return ClassFile.from_fp(fp, decode_full)
