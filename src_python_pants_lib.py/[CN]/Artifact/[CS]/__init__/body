def __init__(self, org, name, repo):
    'org: the originization of this artifact, the group id in maven parlance\n    name: the name of the artifact\n    repo: the repository this artifact is published to'
    self.org = org
    self.name = name
    self.rev = None
    repos = list(repo.resolve())
    if (len(repos) != 1):
        raise Exception(('An artifact must have exactly 1 repo, given: %s' % repos))
    self.repo = repos[0]
