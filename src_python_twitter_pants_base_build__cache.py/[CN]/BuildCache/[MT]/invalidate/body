def invalidate(self, cache_key):
    'Invalidates this cache key and any cached files associated with it.\n\n    :param cache_key: A CacheKey object (as returned by BuildCache.key_for().\n    '
    safe_rmtree(cache_key.filename)
    sha_file = self._sha_file(cache_key)
    if os.path.exists(sha_file):
        os.unlink(sha_file)
