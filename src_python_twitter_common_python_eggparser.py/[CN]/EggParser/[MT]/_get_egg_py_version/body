@staticmethod
def _get_egg_py_version(components):
    if (components and components[0].startswith('py')):
        try:
            (major, minor) = components[0][2:].split('.')
            (major, minor) = (int(major), int(minor))
            return ((major, minor), components[1:])
        except:
            pass
    return ((), components)
