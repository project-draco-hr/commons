def _create_template_data(self):
    (jar_dependency, id, exported) = self._get_artifact_info()
    if self.excludes:
        exclude_template_datas = [exclude._create_template_data() for exclude in self.excludes]
    else:
        exclude_template_datas = None
    return TemplateData(id=id, name=self.name, template_base=self.target_base, exported=exported, org=jar_dependency.org, module=jar_dependency.name, version=jar_dependency.rev, sources=self.sources, dependencies=[dep._create_template_data() for dep in self.jar_dependencies], excludes=exclude_template_datas, buildflags=self.buildflags)
