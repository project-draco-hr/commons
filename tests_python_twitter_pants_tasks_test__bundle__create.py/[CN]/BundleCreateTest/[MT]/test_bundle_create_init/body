def test_bundle_create_init(self):
    options = {'jvm_binary_create_outdir': None, 'binary_create_compressed': None, 'binary_create_zip64': None, 'jvm_binary_create_deployjar': None, 'bundle_create_prefix': None, 'bundle_create_archive': None, }
    bundle_create = BundleCreate(create_context(config=sample_ini_test_1, options=options))
    self.assertEquals(bundle_create.outdir, '/tmp/dist')
