def setup_artifact_cache(self, spec):
    'Subclasses can call this in their __init__() to set up artifact caching for that task type.\n\n    spec should be a list of urls/file path prefixes, which are used in that order.\n    By default, no artifact caching is used.\n    '
    if (len(spec) > 0):
        pants_workdir = self.context.config.getdefault('pants_workdir')
        self._artifact_cache = create_artifact_cache(self.context, pants_workdir, spec)
