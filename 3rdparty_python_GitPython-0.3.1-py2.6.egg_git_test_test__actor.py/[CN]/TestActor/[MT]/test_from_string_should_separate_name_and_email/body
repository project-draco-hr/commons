def test_from_string_should_separate_name_and_email(self):
    a = Actor._from_string('Michael Trier <mtrier@example.com>')
    assert_equal('Michael Trier', a.name)
    assert_equal('mtrier@example.com', a.email)
    assert (a == a)
    assert (not (a != a))
    m = set()
    m.add(a)
    m.add(a)
    assert (len(m) == 1)
