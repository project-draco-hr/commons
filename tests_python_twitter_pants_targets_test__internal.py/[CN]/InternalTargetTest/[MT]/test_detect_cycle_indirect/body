def test_detect_cycle_indirect(self):
    c = MockTarget('c')
    b = MockTarget('b', [c])
    a = MockTarget('a', [c, b])
    InternalTarget.sort_targets([a])
    c.internal_dependencies = [a]
    try:
        InternalTarget.sort_targets([a])
        self.fail('Expected a cycle to be detected')
    except InternalTarget.CycleException:
        pass
