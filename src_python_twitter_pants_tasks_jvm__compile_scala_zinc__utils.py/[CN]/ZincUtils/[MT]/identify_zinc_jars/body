@staticmethod
def identify_zinc_jars(zinc_classpath):
    'Find the named jars in the zinc classpath.\n\n    TODO: Make these mappings explicit instead of deriving them by jar name heuristics.\n    '
    ret = OrderedDict()
    ret.update(ZincUtils.identify_jars(ZincUtils.zinc_jar_names, zinc_classpath))
    return ret
