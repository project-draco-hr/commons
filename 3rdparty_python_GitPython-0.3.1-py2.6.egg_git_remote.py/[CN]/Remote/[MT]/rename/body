def rename(self, new_name):
    'Rename self to the given new_name\n\t\t:return: self '
    if (self.name == new_name):
        return self
    self.repo.git.remote('rename', self.name, new_name)
    self.name = new_name
    try:
        del self._config_reader
    except AttributeError:
        pass
    return self
