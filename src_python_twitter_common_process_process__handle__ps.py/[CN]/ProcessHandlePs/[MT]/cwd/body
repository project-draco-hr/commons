def cwd(self):
    try:
        lsof = subprocess.Popen(('lsof -a -p %s -d cwd -Fn' % self.pid()).split(), stdout=subprocess.PIPE, stderr=subprocess.PIPE)
        (stdout, stderr) = lsof.communicate()
        for line in stdout.split('\n'):
            if line.startswith('n'):
                return line[1:]
    except OSError:
        return None
