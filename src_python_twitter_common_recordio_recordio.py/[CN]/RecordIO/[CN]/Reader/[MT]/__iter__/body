def __iter__(self):
    '\n      Return an iterator over the entire contents of the underlying file handle.\n\n        May raise:\n          RecordIO.PrematureEndOfStream\n      '
    try:
        dup_fp = self._fp.dup()
    except self._fp.Error:
        log.error(('Failed to dup %r' % self._fp))
        return
    try:
        while True:
            blob = RecordIO.Reader.do_read(dup_fp, self._codec)
            if blob:
                yield blob
            else:
                break
    except RecordIO.Error as e:
        log.error(('Caught exception in __iter__: %s' % e))
        dup_fp.close()
