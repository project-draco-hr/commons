def itervalues(self):
    '\n        >>> iv = OrderedDict(((1, 3), (3, 2), (2, 1))).itervalues()\n        >>> iv.next()\n        3\n        >>> iv.next()\n        2\n        >>> iv.next()\n        1\n        >>> iv.next()\n        Traceback (most recent call last):\n        StopIteration\n        '

    def make_iter(self=self):
        keys = self.iterkeys()
        while True:
            yield self[keys.next()]
    return make_iter()
