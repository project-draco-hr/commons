def index(self, key):
    '\n        Return the position of the specified key in the OrderedDict.\n        \n        >>> d = OrderedDict(((1, 3), (3, 2), (2, 1)))\n        >>> d.index(3)\n        1\n        >>> d.index(4)\n        Traceback (most recent call last):\n        ValueError: list.index(x): x not in list\n        '
    return self._sequence.index(key)
