def tags_tocl(d, tag_list, title):
    'Generate specialized TOC.\n  E.g., tags_tocl(d, ["python", "anylang"], "Python")\n  tag_list: if an entry\'s tags contains any of these, use it\n  title: pretty title\n  '
    filtered_anchors = []
    for anc in sorted(d.keys(), key=str.lower):
        entry = d[anc]
        if (not ('tags' in entry)):
            continue
        found = [t for t in tag_list if (t in entry['tags'])]
        if (not found):
            continue
        filtered_anchors.append(anc)
    return TemplateData(t=title, e=filtered_anchors)
