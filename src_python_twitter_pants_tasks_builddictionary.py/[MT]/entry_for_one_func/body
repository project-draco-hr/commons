def entry_for_one_func(nom, func):
    "Generate a BUILD dictionary entry for a function\n  nom: name like 'python_binary'\n  func: function object"
    (args, varargs, varkw, defaults) = inspect.getargspec(func)
    argspec = inspect.formatargspec(args, varargs, varkw, defaults)
    return entry(nom, argspec=argspec, funcdoc=func.__doc__)
