{
  out=new StringWriter();
  err=new StringWriter();
  listener=new AnsiColorDiagnosticListener<FileObject>(new PrintWriter(out),new PrintWriter(err));
  listener.prepareConsole(false);
  addTearDown(new TearDown(){
    @Override public void tearDown(){
      listener.releaseConsole();
    }
  }
);
  diagnostic=createMock(new Clazz<Diagnostic<FileObject>>(){
  }
);
  file=createMock(FileObject.class);
}
