@classmethod
def setUpClass(self):
    super(TestPythonThriftBuilder, self).setUpClass()
    SourceRoot.register(os.path.realpath(os.path.join(self.build_root, 'test_thrift_replacement')), PythonThriftLibrary)
    self.create_target('test_thrift_replacement', dedent("\n      python_thrift_library(name='one',\n        sources=['thrift/keyword.thrift'],\n        dependencies=None\n      )\n    "))
