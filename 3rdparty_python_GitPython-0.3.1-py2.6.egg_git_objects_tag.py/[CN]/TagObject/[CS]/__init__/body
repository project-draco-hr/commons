def __init__(self, repo, binsha, object=None, tag=None, tagger=None, tagged_date=None, tagger_tz_offset=None, message=None):
    'Initialize a tag object with additional data\n\t\t\n\t\t:param repo: repository this object is located in\n\t\t:param binsha: 20 byte SHA1\n\t\t:param object: Object instance of object we are pointing to\n\t\t:param tag: name of this tag\n\t\t:param tagger: Actor identifying the tagger\n\t\t:param tagged_date: int_seconds_since_epoch\n\t\t\tis the DateTime of the tag creation - use time.gmtime to convert \n\t\t\tit into a different format\n\t\t:param tagged_tz_offset: int_seconds_west_of_utc is the timezone that the \n\t\t\tauthored_date is in, in a format similar to time.altzone'
    super(TagObject, self).__init__(repo, binsha)
    if (object is not None):
        self.object = object
    if (tag is not None):
        self.tag = tag
    if (tagger is not None):
        self.tagger = tagger
    if (tagged_date is not None):
        self.tagged_date = tagged_date
    if (tagger_tz_offset is not None):
        self.tagger_tz_offset = tagger_tz_offset
    if (message is not None):
        self.message = message
