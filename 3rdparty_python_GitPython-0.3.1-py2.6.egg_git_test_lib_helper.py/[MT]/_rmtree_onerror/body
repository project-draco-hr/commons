def _rmtree_onerror(osremove, fullpath, exec_info):
    '\n\tHandle the case on windows that read-only files cannot be deleted by \n\tos.remove by setting it to mode 777, then retry deletion.\n\t'
    if ((os.name != 'nt') or (osremove is not os.remove)):
        raise
    os.chmod(fullpath, 511)
    os.remove(fullpath)
