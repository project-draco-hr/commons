def __getitem__(self, filename):
    info = _zipfile_cache[self._archive].getinfo(filename.replace(os.sep, '/'))
    dt = info.date_time
    dostime = (((dt[3] << 11) | (dt[4] << 5)) | (dt[5] // 2))
    dosdate = ((((dt[0] - 1980) << 9) | (dt[1] << 5)) | dt[2])
    return (os.path.join(self._archive, info.filename), info.compress_type, info.compress_size, info.file_size, info.header_offset, dostime, dosdate, info.CRC)
