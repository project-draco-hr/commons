@staticmethod
def identify_zinc_jars(compiler_classpath, zinc_classpath):
    'Find the named jars in the compiler and zinc classpaths.\n\n    TODO: When profiles migrate to regular pants jar() deps instead of ivy.xml files we can make these\n          mappings explicit instead of deriving them by jar name heuristics.\n    '
    ret = OrderedDict()
    ret.update(ZincUtils.identify_jars(ZincUtils.compiler_jar_names, compiler_classpath))
    ret.update(ZincUtils.identify_jars(ZincUtils.zinc_jar_names, zinc_classpath))
    return ret
