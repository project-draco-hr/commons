@contextmanager
def temporary(self, path):
    'A contextmanager that establishes a temporary ROOT_DIR, restoring the prior ROOT_DIR on\n    exit.'
    if (path is None):
        raise ValueError('Can only temporarily establish a build root given a path.')
    prior = self._root_dir
    self._root_dir = path
    try:
        yield
    finally:
        self._root_dir = prior
