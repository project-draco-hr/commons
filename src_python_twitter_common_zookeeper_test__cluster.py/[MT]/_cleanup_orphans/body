@atexit.register
def _cleanup_orphans():
    for pid in ZookeeperClusterBootstrapper._orphaned_pids:
        try:
            os.kill(pid, signal.SIGKILL)
        except OSError as e:
            if (e.errno != errno.ESRCH):
                print  >> sys.stderr, ('warning: error killing orphaned ZK server %d: %s' % (pid, e))
