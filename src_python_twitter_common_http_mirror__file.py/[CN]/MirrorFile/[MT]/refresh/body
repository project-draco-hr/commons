def refresh(self):
    '\n      Refresh the local file if necessary.  Returns truthy if the underlying file changed.\n    '
    self._local_mtime = self._get_local_timestamp()
    self._web_mtime = self._get_web_timestamp()
    if (self._web_mtime is None):
        return None
    elif (self._web_mtime != self._local_mtime):
        return self._fetch()
