def get_all(self):
    'Returns all the timings, sorted in decreasing order.\n\n    Each value is a dict: { path: <path>, timing: <timing in seconds> }\n    '
    return [{'label': x[0], 'timing': x[1], 'is_tool': (x[0] in self._tool_labels), } for x in sorted(self._timings_by_path.items(), key=(lambda x: x[1]), reverse=True)]
