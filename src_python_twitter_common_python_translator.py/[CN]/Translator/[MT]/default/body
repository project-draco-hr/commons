@staticmethod
def default(install_cache=None, platform=Platform.current(), interpreter=PythonInterpreter.get(), conn_timeout=None):
    return ChainedTranslator(EggTranslator(install_cache=install_cache, platform=platform, python=interpreter.python, conn_timeout=conn_timeout), SourceTranslator(install_cache=install_cache, interpreter=interpreter, conn_timeout=conn_timeout))
