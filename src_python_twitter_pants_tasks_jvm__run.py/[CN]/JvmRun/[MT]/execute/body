def execute(self, targets):
    binaries = filter(is_binary, targets)
    if (len(binaries) > 0):
        main = binaries[0].main
        result = runjava(jvmargs=self.jvm_args, classpath=self.classpath(confs=self.confs), main=main, args=self.args)
        if (result != 0):
            raise TaskError()
