def extract_jvm_targets(targets):
    'Returns an iterator over the jvm targets the given sequence of targets resolve to.  The given\n  targets can be a mix of types and any non jvm targets (as determined by is_jvm(...) will be\n  filtered out from the returned iterator.'
    for target in targets:
        if (target is None):
            print('Warning! Null target!', file=sys.stderr)
            continue
        for real_target in target.resolve():
            if is_jvm(real_target):
                yield real_target
