def extract_target(java_targets, is_transitive, name=None):
    meta_target = bang.extract_target(java_targets, name)
    (internal_deps, jar_deps) = _extract_target(meta_target, is_transitive, (lambda target: is_apt(target)))
    sources = ['__no_source__']
    all_deps = OrderedSet()
    all_deps.update(internal_deps)
    all_deps.update(jar_deps)
    return JavaLibrary('ide', sources, dependencies=all_deps, excludes=meta_target.excludes, is_meta=True)
