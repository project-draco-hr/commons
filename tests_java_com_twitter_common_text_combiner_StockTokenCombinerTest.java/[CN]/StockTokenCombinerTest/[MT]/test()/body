{
  stream.reset("Check out Apple stock: $APPL. and $IN:REL.  But not $dlfsdfsdf. $C is ok. $ is right out.");
  assertEquals(ImmutableList.of("Check","out","Apple","stock",":","$APPL",".","and","$IN:REL",".","But","not","$","dlfsdfsdf",".","$C","is","ok",".","$","is","right","out","."),stream.toStringList());
}
