{
  int argCount=compiler.isSupportedOption(arg);
  if (argCount == -1) {
    argCount=standardFileManager.isSupportedOption(arg);
  }
  if (argCount == -1) {
    System.err.println("WARNING: Skipping unsupported option " + arg);
  }
 else {
    options.add(arg);
    while (argCount-- > 0) {
      if (iter.hasNext()) {
        options.add(iter.next());
      }
    }
  }
}
