def execute(self, targets):
    self.context.lock.release()
    self.save_stty_options()
    try:
        runjava_indivisible(jvmargs=self.jvm_args, classpath=self.classpath(profile_classpath(self.profile), confs=self.confs), main=self.main, args=self.args, workunit_factory=self.context.new_workunit, workunit_name='repl')
    except KeyboardInterrupt:
        pass
    self.restore_ssty_options()
