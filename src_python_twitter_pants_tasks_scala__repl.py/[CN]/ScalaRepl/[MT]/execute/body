def execute(self, targets):
    self.context.lock.release()
    self.save_stty_options()

    def repl_workunit_factory(name, labels=list(), cmd=''):
        return self.context.new_workunit(name=name, labels=([WorkUnit.REPL] + labels), cmd=cmd)
    try:
        runjava_indivisible(jvmargs=self.jvm_args, classpath=self.classpath(profile_classpath(self.profile), confs=self.confs), main=self.main, args=self.args, workunit_factory=repl_workunit_factory, workunit_name='repl')
    except KeyboardInterrupt:
        pass
    self.restore_ssty_options()
