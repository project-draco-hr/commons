def execute(self, targets):
    tools_classpath = self._jvm_tool_bootstrapper.get_jvm_tool_classpath(self._bootstrap_key)
    self.context.lock.release()
    self.save_stty_options()
    classpath = self.classpath(tools_classpath, confs=self.confs, exclusives_classpath=self.get_base_classpath_for_target(targets[0]))
    print ''
    try:
        execute_java(classpath=classpath, main=self.main, jvm_options=self.jvm_args, args=self.args, workunit_labels=[WorkUnit.REPL, WorkUnit.JVM], workunit_name='repl')
    except KeyboardInterrupt:
        pass
    self.restore_ssty_options()
