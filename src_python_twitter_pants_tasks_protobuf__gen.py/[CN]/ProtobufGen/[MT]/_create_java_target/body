def _create_java_target(self, target, dependees):
    genfiles = []
    for source in target.sources:
        path = os.path.join(target.target_base, source)
        genfiles.extend(calculate_genfiles(path, source).get('java', []))
    tgt = self.context.add_new_target(self.java_out, JavaLibrary, name=target.id, provides=target.provides, sources=genfiles, dependencies=self.javadeps)
    tgt.id = target.id
    tgt.add_label('codegen')
    for dependee in dependees:
        dependee.update_dependencies([tgt])
    return tgt
