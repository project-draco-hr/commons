from twitter.pants.goal.products import Products
try:
    import psutil

    def _process_info(pid):
        process = psutil.Process(pid)
        return ('%d (%s)' % (pid, ' '.join(process.cmdline)))
except ImportError:

    def _process_info(pid):
        return ('%d' % pid)
