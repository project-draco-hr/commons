def test_singleton_names():


    class Hello(object):
        __metaclass__ = SingletonMetaclass

        def __init__(self, sig):
            self._sig = sig
    H1 = Hello


    class Hello(object):
        __metaclass__ = SingletonMetaclass

        def __init__(self, sig):
            self._sig = sig
    H2 = Hello
    h1 = H1('a')
    h2 = H2('b')
    assert (id(h1) != id(h2)), 'class names should not matter with singleton decorator'
    assert (h1._sig != h2._sig)
