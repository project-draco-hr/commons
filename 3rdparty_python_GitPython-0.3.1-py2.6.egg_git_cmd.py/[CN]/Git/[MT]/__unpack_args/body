@classmethod
def __unpack_args(cls, arg_list):
    if (not isinstance(arg_list, (list, tuple))):
        return [str(arg_list)]
    outlist = list()
    for arg in arg_list:
        if isinstance(arg_list, (list, tuple)):
            outlist.extend(cls.__unpack_args(arg))
        else:
            outlist.append(str(arg))
    return outlist
