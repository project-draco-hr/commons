def __getattr__(self, name):
    'A convenience method as it allows to call the command as if it was \n\t\tan object.\n\t\t:return: Callable object that will execute call _call_process with your arguments.'
    if (name[:1] == '_'):
        raise AttributeError(name)
    return (lambda *args, **kwargs: self._call_process(name, *args, **kwargs))
