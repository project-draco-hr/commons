def readlines(self, size=(-1)):
    if (self._nbr == self._size):
        return list()
    out = list()
    nbr = 0
    while True:
        line = self.readline()
        if (not line):
            break
        out.append(line)
        if (size > (-1)):
            nbr += len(line)
            if (nbr > size):
                break
    return out
