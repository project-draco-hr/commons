def __init__(self, name, username=None, password=None):
    '\n      :name The name of these credentials\n      :username Either a constant username value or else a callable that can fetch one\n      :password Either a constant password value or else a callable that can fetch one\n    '
    Target.__init__(self, name)
    self._username = (username if callable(username) else (lambda : username))
    self._password = (password if callable(password) else (lambda : password))
