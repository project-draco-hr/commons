def exists(self):
    "\n\t\t:return: True if the submodule exists, False otherwise. Please note that\n\t\t\ta submodule may exist (in the .gitmodules file) even though its module\n\t\t\tdoesn't exist"
    loc = locals()
    for attr in self._cache_attrs:
        if hasattr(self, attr):
            loc[attr] = getattr(self, attr)
    self._clear_cache()
    try:
        try:
            self.path
            return True
        except Exception:
            return False
    finally:
        for attr in self._cache_attrs:
            if (attr in loc):
                setattr(self, attr, loc[attr])
