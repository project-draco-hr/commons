def __init__(self, name, dependencies):
    'name: The name of this module target, addressable via pants via the portion of the spec\n        following the colon\n    dependencies: one or more JarDependencies this JarLibrary bundles or Pants pointing to other\n        JarLibraries or JavaTargets'
    assert (len(dependencies) > 0), 'At least one dependency must be specified'
    Target.__init__(self, name, False)
    self.dependencies = dependencies
