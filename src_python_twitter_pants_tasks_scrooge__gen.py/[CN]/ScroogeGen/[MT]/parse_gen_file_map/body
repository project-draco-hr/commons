def parse_gen_file_map(self, gen_file_map, outdir):
    d = defaultdict(set)
    with open(gen_file_map, 'r') as deps:
        for dep in deps:
            (src, cls) = dep.strip().split('->')
            src = os.path.relpath(src.strip(), os.path.curdir)
            cls = os.path.relpath(cls.strip(), outdir)
            d[src].add(cls)
    return d
