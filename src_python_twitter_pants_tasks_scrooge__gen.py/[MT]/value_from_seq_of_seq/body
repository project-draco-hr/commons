def value_from_seq_of_seq(seq_of_seq, key, default=None):
    result = default
    for seq in seq_of_seq:
        if ((len(seq) == 1) and (key == seq[0])):
            break
        elif ((len(seq) == 2) and (key == seq[0])):
            result = seq[1]
            break
        elif (len(seq) == 0):
            raise ValueError('A sequence of sequences may not have less than one element in a sub-sequence.')
        elif (len(seq) > 2):
            raise ValueError('A sequence of sequences may not have more than two elements in a sub-sequence.')
    return result
